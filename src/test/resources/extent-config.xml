<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <theme>standard</theme>
        
        <!-- document encoding -->
        <encoding>UTF-8</encoding>
        
        <!-- protocol for script and stylesheets -->
        <protocol>https</protocol>
        
        <!-- title of the document -->
        <documentTitle>UI-API Validation Report</documentTitle>
        
        <!-- report name - displayed at top-nav -->
        <reportName>Automation Test Report</reportName>
        
        <!-- report headline - displayed at top-nav, after reportName -->
        <reportHeadline></reportHeadline>
        
        <!-- global date format override -->
        <dateFormat>MMM dd, yyyy HH:mm:ss</dateFormat>
        
        <!-- enable timeline in test view -->
        <timelineEnabled>true</timelineEnabled>
        
        <!-- default chart visibility -->
        <chartVisibilityOnOpen>true</chartVisibilityOnOpen>
        
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                });
            ]]>
        </scripts>
        
        <!-- custom styles -->
        <styles>
            <![CDATA[
                
            ]]>
        </styles>
    </configuration>
</extentreports>